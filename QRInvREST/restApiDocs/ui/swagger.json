    "\/generator/image": {
      "get": {
        "tags": [
          "QRInvoiceGeneratorResource"
        ],
        "description": "generate QR code based on invoice data",
        "operationId": "resource_QRInvoiceGeneratorResource_generateInvoiceQR_GET",
        "produces": [
          "image/png"
        ],
        "parameters": [
          {
            "name": "ACC",
            "in": "query",
            "type": "string",
            "description": "IBAN number"
          },
          {
            "name": "AM",
            "in": "query",
            "type": "string",
            "description": "Total amount to pay in @see CC currency"
          },
          {
            "name": "BIC",
            "in": "query",
            "type": "string",
            "description": "BIC code of bank"
          },
          {
            "name": "CC",
            "in": "query",
            "default": "CZK",
            "type": "string",
            "description": "Currency of total amount"
          },
          {
            "name": "DD",
            "in": "query",
            "type": "string",
            "description": "Date of issue of invoice"
          },
          {
            "name": "DPPD",
            "in": "query",
            "type": "string",
            "description": "Date of duty to declare tax"
          },
          {
            "name": "DT",
            "in": "query",
            "type": "string",
            "description": "Date of due of total amount"
          },
          {
            "name": "DUZP",
            "in": "query",
            "type": "string",
            "description": "Date of taxable event"
          },
          {
            "name": "FX",
            "in": "query",
            "type": "string",
            "description": "Exchange rate between CZK currency and currency of total amount"
          },
          {
            "name": "FXA",
            "in": "query",
            "default": "1",
            "type": "string",
            "description": "Currency units"
          },
          {
            "name": "INI",
            "in": "query",
            "type": "string",
            "description": "Identification number of issuer - IČO"
          },
          {
            "name": "INR",
            "in": "query",
            "type": "string",
            "description": "Identification number of recipient - IČO"
          },
          {
            "name": "MSG",
            "in": "query",
            "type": "string",
            "description": "Text description of subject of invoice"
          },
          {
            "name": "NTB",
            "in": "query",
            "type": "string",
            "description": "Amount of non taxable event"
          },
          {
            "name": "ON",
            "in": "query",
            "type": "string",
            "description": "Number of order"
          },
          {
            "name": "SA",
            "in": "query",
            "default": "0",
            "type": "string",
            "description": "Flag if tax advance payments are on invoice"
          },
          {
            "name": "SPAYD-MSG",
            "in": "query",
            "type": "string",
            "description": "Message for beneficiary from spayd specification"
          },
          {
            "name": "SPAYD-URL",
            "in": "query",
            "type": "string",
            "description": "URL from spayd specification"
          },
          {
            "name": "T0",
            "in": "query",
            "type": "string",
            "description": "Tax amount in base tax rate"
          },
          {
            "name": "T1",
            "in": "query",
            "type": "string",
            "description": "Tax amount in first reduced rate"
          },
          {
            "name": "T2",
            "in": "query",
            "type": "string",
            "description": "Tax amount in second reduced rate"
          },
          {
            "name": "TB0",
            "in": "query",
            "type": "string",
            "description": "Tax base"
          },
          {
            "name": "TB1",
            "in": "query",
            "type": "string",
            "description": "Tax base in first reduced rate"
          },
          {
            "name": "TB2",
            "in": "query",
            "type": "string",
            "description": "Tax base in second reduced rate"
          },
          {
            "name": "TD",
            "in": "query",
            "type": "string",
            "description": "Type of tax document"
          },
          {
            "name": "TP",
            "in": "query",
            "default": "0",
            "type": "string",
            "description": "Identification of type of taxable event"
          },
          {
            "name": "VII",
            "in": "query",
            "type": "string",
            "description": "Tax identification number of issuer - DIČ"
          },
          {
            "name": "VIR",
            "in": "query",
            "type": "string",
            "description": "Tax identification number of recipient - DIČ"
          },
          {
            "name": "VS",
            "in": "query",
            "type": "string",
            "description": "Variable symbol"
          },
          {
            "name": "X-SW",
            "in": "query",
            "type": "string",
            "description": "Identification of Software"
          },
          {
            "name": "X-URL",
            "in": "query",
            "type": "string",
            "description": "URL to electronic document of invoice"
          },
          {
            "name": "accountBase",
            "in": "query",
            "type": "string",
            "description": "Base of account number"
          },
          {
            "name": "accountPrefix",
            "in": "query",
            "type": "string",
            "description": "Account number prefix"
          },
          {
            "name": "bankCode",
            "in": "query",
            "type": "string",
            "description": "Code of bank"
          },
          {
            "name": "branding",
            "in": "query",
            "default": "true",
            "type": "boolean",
            "description": "if resulted image has to have branding labels"
          },
          {
            "name": "id",
            "in": "query",
            "type": "string",
            "description": "ID of generated invoice"
          },
          {
            "name": "mode",
            "in": "query",
            "default": "INVOICE_MODE",
            "type": "string",
            "enum": [
              "INVOICE_MODE",
              "SPAYD_MODE"
            ],
            "description": "indicates which mode of QR should be used, pure QR invoice or integrated with SPAYD"
          },
          {
            "name": "size",
            "in": "query",
            "type": "integer",
            "description": ""
          },
          {
            "name": "transliterate",
            "in": "query",
            "default": "true",
            "type": "boolean",
            "description": "identifies if returned string has to have capitalized letters"
          }
        ],
        "responses": {
          "200": {
            "headers": {
            },
            "description": "Success"
          },
          "default": {
            "description": "Unexpected error."
          }
        }
      }
    },
